{
  "name": "zoey-music-manage",
  "version": "1.1.0",
  "main": "dist-electron/main/index.js",
  "description": "zoey music manage",
  "author": "sxgan <sxgan@foxmail.com>",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:9999/"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "vite --mode dev",
    "build": "vue-tsc --noEmit && vite build --mode pro && electron-builder",
    "dev:win": "chcp 65001 && vite --mode dev",
    "build:win": "chcp 65001 && vue-tsc --noEmit && vite build --mode pro && electron-builder",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.1",
    "axios": "^1.6.7",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "tree-kill": "^1.2.2",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.10",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.4.1",
    "vue-tsc": "^1.8.27"
  },
  "dependencies": {
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "sass": "^1.71.0",
    "vue-router": "^4.2.5"
